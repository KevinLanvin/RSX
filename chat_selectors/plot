set title "Evolution of CPU time used with increasing amount of data"
set xrange [1000:1000000]
set logscale x
set xlabel "Number of sent bytes"
set yrange [0.01:5]
set ylabel "CPU time used"
set terminal svg enhanced background rgb 'white' fname 'graph'
set output 'graph'

plot "results.txt" u 1:2 title '1 by 1' with linespoints, "results.txt" u 1:3 title 'All in one' with linespoints
